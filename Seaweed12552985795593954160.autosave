class Seaweed extends Animal{

//Constuctor
    Seaweed(int a, int h, PVector s, PVector p, int v, int r){
        super(a, h, s, p, v, r, "Seaweed");

    }

//Methods

//Draw me method

void drawMe(){
  fill(this.status);
  noStroke();
  circle(this.pos.x, this.pos.y, diameter);
  
  if (beingHunt)
    run();
    
  else {
    rest();
  }  
  
  
////////WILL BE DONE WITH VISUALS

}

//Eating method

void eat(){
this.hunger += 1;
    
}

}
